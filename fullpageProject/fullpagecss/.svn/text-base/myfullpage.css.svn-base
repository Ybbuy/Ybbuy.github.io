/*第四屏*/
.section strong { position: absolute; left: 50%; top: 30%; z-index: 10; width: 512px; height: 45px; margin-left: -256px; font-size: 24px; font-weight: 500; color: #333; opacity: 0; -webkit-transform:scale(0.5, 0.5); transform:scale(0.5, 0.5); transition: all 1s;}
.section h3 {font-size:30px; width: 512px; height: 45px; margin-left: -256px; margin-top: 60px; color: #8A2BE2;position: absolute; left: 50%; top: 10%; z-index: 20;  opacity: 0; }
.p1 { position: absolute; left: 50%; top: 20%; width: 510px; margin-left: -255px; margin-top: 130px; font-size: 16px; color: #474c51; opacity: 0; transition: all 1s;}
.bg41 { position: absolute; bottom: -372px; width: 100%; height: 372px; background: url(../images/bg41.png) 50% 0 no-repeat; transition: all 1s;}
.bg42 { position: absolute; bottom: -449px; width: 100%; height: 449px; background: url(../images/bg42.png) 50% 0 no-repeat; transition: all 1s;}
.bg43 { position: absolute; bottom: -449px; width: 100%; height: 449px; margin-bottom: -130px; background: url(../images/bg43.png) 50% 0 no-repeat; transition: all 1s;}
.active .bg41 { bottom: 15%; transition-delay: 0.7s;}
.active .bg42 { bottom: 15%; transition-delay: 1.1s;}
.active .bg43 { bottom: 15%; transition-delay: 0.9s;}
.active strong, .active h3 { opacity: 1; -webkit-transform:scale(1, 1); transform:scale(1, 1); transition-delay: 0.7s;}
.active .p1 { opacity: 1; transition-delay: 1.7s;}

/*第五屏*/
.section5 h4 {font-size:30px; width: 512px; height: 45px; margin-left: -256px; margin-top: 60px; color: #EAEFF2;position: absolute; left: -50%; top: 3%; z-index: 20; }
.section5 p {font-size:20px; width: 512px; height: 45px; margin-left: -256px; margin-top: 60px; color: #EAEFF2;position: absolute; right: -50%; top: 15%; z-index: 20; }
.section5 .circle{ position:absolute;width:595px;height:357px;left:50%;top:150px;margin-left:-293px;z-index:30;}
.section5 .circle div{background:url(../images/circle1.png) no-repeat 50% 50%;background-size:35% 35%;position:absolute;width:100%;height:100%;opacity:0;}
.section5 .circle_a{animation:ani-circle-a 4.5s ease-out 1s forwards infinite;-webkit-animation:ani-circle-a 4.5s ease-out forwards infinite;}
.section5 .circle_b{animation:ani-circle-a 4.5s ease-out 1.5s forwards infinite;-webkit-animation:ani-circle-a 4.5s ease-out 1.5s forwards infinite;}
.section5 .circle_c{animation:ani-circle-a 4.5s ease-out 3s forwards infinite;-webkit-animation:ani-circle-a 4.5s ease-out 3s forwards infinite;}

@-webkit-keyframes ani-circle-a{0%{-webkit-transform:scale(0.8);transform:scale(0.8);opacity:0;filter:alpha(opacity=0)\9}10%{opacity:1;filter:alpha(opacity=100)\9}100%{-webkit-transform:scale(3.5);transform:scale(3.5);opacity:0;filter:alpha(opacity=0)\9}}@keyframes ani-circle-a{0%{-webkit-transform:scale(0.8);transform:scale(0.8);opacity:0;filter:alpha(opacity=0)\9}10%{opacity:1;filter:alpha(opacity=100)\9}100%{-webkit-transform:scale(3.5);transform:scale(3.5);opacity:0;filter:alpha(opacity=0)\9}}

